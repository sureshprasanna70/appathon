<h1>Listing submodules</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Desc</th>
      <th>Deadline</th>
      <th>Score</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @submodules.each do |submodule| %>
      <tr>
        <td><%= submodule.name %></td>
        <td><%= submodule.desc %></td>
        <td><%= submodule.deadline %></td>
        <td><%= submodule.score %></td>
        <td><%= link_to 'Show', submodule %></td>
        <td><%= link_to 'Edit', edit_submodule_path(submodule) %></td>
        <td><%= link_to 'Destroy', submodule, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Submodule', new_submodule_path %>
